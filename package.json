{
  "name": "cognito-rest-api",
  "version": "0.0.0",
  "description": "A rest API wrapper for AWS Cognito using typescript and TSOA",
  "main": "dist/app.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "tsc",
    "test": "node_modules/.bin/mocha dist/**/*.spec.js --reporter=list",
    "e2e": "node_modules/.bin/mocha dist/test/*.e2e.js -b --exit --timeout=60000 --reporter=list",
    "start": "pm2 start dist/index.js --name app",
    "dev-debug": "nodemon -e ts -w ./src -x ts-node --inspect=5858 src/index.ts",
    "dev": "nodemon -e ts -w ./src -x ts-node src/index.ts",
    "swagger": "node_modules/.bin/tsoa swagger",
    "routes": "node_modules/.bin/tsoa routes",
    "preversion": "npm build && npm run lint",
    "version": "git add -A .",
    "postversion": "git push && git push --tags"
  },
  "author": "PGowar",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.16.7",
    "@types/chai": "^4.0.6",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.0.39",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/mocha": "^2.2.44",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.10.29",
    "@types/request-promise": "^4.1.39",
    "@types/supertest": "^2.0.6",
    "archiver": "^2.1.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "mailslurp-client": "0.1.1514850454",
    "minimist": "^1.2.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.18.4",
    "sinon-express-mock": "^1.3.1",
    "supertest": "^3.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "aws-param-store": "^2.0.0",
    "aws-sdk": "^2.315.0",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-http-context": "^1.0.4",
    "helmet": "^3.9.0",
    "injection-js": "^2.2.1",
    "jsonwebtoken": "^8.3.0",
    "jwk-to-pem": "^2.0.0",
    "memcached": "^2.2.2",
    "morgan": "^1.9.1",
    "mysql-client": "https://s3.eu-west-2.amazonaws.com/paul-gowar-public/mysql-client-1.0.0.tgz",
    "reflect-metadata": "^0.1.12",
    "swagger-ui-express": "^3.0.10",
    "tsoa": "2.1.5",
    "uuid": "^3.3.2",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.3.2"
  }
}
